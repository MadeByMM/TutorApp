<?xml version="1.0" encoding="utf-8"?>

<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.tutorapp.Home"
        android:label="Hjem"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_toolbox"
        android:name="com.example.tutorapp.Toolbox"
        android:label="Værktøjskassen"
        tools:layout="@layout/fragment_toolbox" />

    <fragment
        android:id="@+id/navigation_terms"
        android:name="com.example.tutorapp.Terms"
        android:label="Tutorbetingelser"
        tools:layout="@layout/fragment_terms" />

    <fragment
        android:id="@+id/navigation_bookshelf"
        android:name="com.example.tutorapp.Bookshelf"
        android:label="Bogreolen"
        tools:layout="@layout/fragment_bookshelf" />

    <fragment
        android:id="@+id/navigation_events"
        android:name="com.example.tutorapp.Events"
        android:label="Event Børs"
